<odoo>
    <record id="view_partner_form_inherit_saes" model="ir.ui.view">
        <field name="name">res.partner.form.saes</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="Cuenta Auxiliar">
                    <group>
                        <field name="x_aux_account"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
