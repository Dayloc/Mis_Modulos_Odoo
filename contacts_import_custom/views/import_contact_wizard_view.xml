<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_import_contact_wizard" model="ir.ui.view">
        <field name="name">wizard.import.contact.form</field>
        <field name="model">wizard.import.contact</field>
        <field name="arch" type="xml">
            <form string="Importar Contactos desde Excel">
                <group>
                    <field name="file" filename="filename"/>
                    <field name="filename" invisible="1"/>
                </group>
                <footer>
                    <button string="Cancelar" class="btn-secondary" special="cancel"/>
                    <button string="Importar" type="object" name="action_import" class="btn-primary"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>
